<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.servon.hhu_map">
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="15" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/hhulogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hhulogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.servon.hhu_map.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.servon.hhu_map.MainActivity"></activity>

        <activity android:name="com.servon.hhu_map.ScaleActivity"></activity>

        <activity android:name="com.servon.hhu_map.RoundMapActivity"></activity>

        <activity android:name="com.servon.hhu_map.MaleDormActivity"></activity>

        <activity android:name="com.servon.hhu_map.FemaleDormActivity"></activity>

        <activity android:name="com.servon.hhu_map.LibraryActivity"></activity>

        <activity android:name="com.servon.hhu_map.SecretActivity"></activity>

    </application>

</manifest>